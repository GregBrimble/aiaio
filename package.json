{
	"$schema": "https://www.raycast.com/schemas/extension.json",
	"name": "aiaio",
	"version": "0.0.0",
	"private": true,
	"description": "The Raycast extension description.",
	"homepage": "https://example.com/",
	"type": "module",
	"main": "./mobile/index.ts",
	"bin": "./cli/bin/index.ts",
	"workspaces": [
		"./lib/@aiaio/internal/",
		"./lib/@aviation/context/",
		"./lib/@aviation/destinations/",
		"./lib/@aviation/router/"
	],
	"scripts": {
		"agent:server": "docker compose -f ./.agent/docker-compose.yml up -d",
		"postagent:server": "node ./scripts/configure-opencode-project.ts",
		"preagent:tui": "npm run agent:server",
		"agent:tui": "opencode attach http://localhost:4096",
		"preagent:web": "npm run agent:server",
		"agent:web": "open http://localhost:4096/",
		"prebuild": "concurrently \"npm:prebuild:*\" --group",
		"build": "concurrently \"npm:build:*\" --group",
		"build:raycast": "ray build",
		"build:web": "vite build",
		"prebuild-all": "concurrently \"npm:prebuild:*\" --group",
		"build-all": "concurrently \"npm:build:*\" \"npm:build-all:*\" --group",
		"build-all:android": "eas build --local --platform=android",
		"build-all:ios": "eas build --local --platform=ios",
		"precheck": "npm run build",
		"check": "concurrently \"npm:check:*\" \"npm:test:ci\" --group",
		"check:format": "npm run prettier -- --check",
		"check:lint": "npm run eslint",
		"precheck:types": "npm run generate-types",
		"check:types": "tsgo -b",
		"predeploy": "npm run build",
		"deploy": "wrangler deploy",
		"dev": "concurrently \"npm:dev:*\"",
		"dev:mobile": "npm run expo -- start -g",
		"predev:raycast": "npm run build",
		"dev:raycast": "ray develop",
		"predev:web": "npm run build",
		"dev:web": "vite",
		"preeslint": "npm run generate-types",
		"eslint": "eslint . --cache",
		"expo": "EXPO_NO_TYPESCRIPT_SETUP=1 expo",
		"fix": "concurrently \"npm:fix:*\" --group",
		"fix:format": "npm run prettier -- --write",
		"fix:lint": "npm run eslint -- --fix",
		"generate-types": "wrangler types",
		"prebuild:icons": "node ./scripts/generate-icons.ts",
		"prebuild:workspaces": "npm run build --workspaces --if-present",
		"prepare": "patch-package && husky && playwright install",
		"postprepare": "npm run prebuild",
		"prettier": "prettier . --cache",
		"prepreview": "npm run build",
		"preview": "wrangler dev",
		"ralph:afk": "./.agent/afk-ralph.sh",
		"ralph:once": "./.agent/ralph-once.sh",
		"ralph:prompt": "./.agent/prompt-ralph.sh",
		"test": "npm run vitest",
		"test:ci": "npm run vitest -- run --browser.headless",
		"previtest": "npm run build",
		"vitest": "vitest"
	},
	"prettier": {
		"plugins": [
			"prettier-plugin-packagejson",
			"@trivago/prettier-plugin-sort-imports",
			"prettier-plugin-tailwindcss"
		]
	},
	"overrides": {
		"@vitejs/plugin-rsc": "0.5.19",
		"eslint": "10.0.0",
		"vitest": "4.1.0-beta.4"
	},
	"dependencies": {
		"@cloudflare/playwright": "https://pkg.pr.new/cloudflare/playwright/@cloudflare/playwright@120",
		"@effect/platform-node": "4.0.0-beta.0",
		"@fontsource-variable/inter": "5.2.8",
		"@headlessui/react": "2.2.9",
		"@heroicons/react": "2.2.0",
		"@raycast/api": "1.104.6",
		"@raycast/utils": "2.2.2",
		"capnweb": "https://pkg.pr.new/cloudflare/capnweb@135",
		"clsx": "2.1.1",
		"effect": "4.0.0-beta.0",
		"expo": "54.0.33",
		"expo-dev-client": "6.0.20",
		"expo-status-bar": "3.0.9",
		"react": "19.2.4",
		"react-dom": "19.2.4",
		"react-native": "0.81.5",
		"react-server-dom-webpack": "19.2.4",
		"rsc-html-stream": "0.0.7"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "1.25.0",
		"@cloudflare/vitest-pool-workers": "https://pkg.pr.new/@cloudflare/vitest-pool-workers@11632",
		"@eslint/compat": "2.0.2",
		"@eslint/js": "10.0.1",
		"@raycast/eslint-plugin": "2.1.1",
		"@tailwindcss/vite": "4.1.18",
		"@trivago/prettier-plugin-sort-imports": "6.0.2",
		"@types/eslint-plugin-jsx-a11y": "6.10.1",
		"@types/ignore-walk": "4.0.3",
		"@types/micromatch": "4.0.10",
		"@types/node": "25.2.3",
		"@types/react": "19.2.14",
		"@types/react-dom": "19.2.3",
		"@typescript/native-preview": "7.0.0-dev.20260212.1",
		"@vitejs/plugin-react": "5.1.4",
		"@vitejs/plugin-rsc": "0.5.19",
		"@vitest/browser-playwright": "4.1.0-beta.4",
		"@vitest/coverage-istanbul": "4.1.0-beta.4",
		"babel-plugin-react-compiler": "1.0.0",
		"concurrently": "9.2.1",
		"eas-cli": "16.32.0",
		"eslint": "10.0.0",
		"eslint-plugin-jsx-a11y": "6.10.2",
		"eslint-plugin-react": "7.37.5",
		"eslint-plugin-react-compiler": "19.1.0-rc.2",
		"eslint-plugin-react-hooks": "7.0.1",
		"husky": "9.1.7",
		"ignore-walk": "8.0.0",
		"lint-staged": "16.2.7",
		"micromatch": "4.0.8",
		"patch-package": "8.0.1",
		"playwright": "1.58.2",
		"prettier": "3.8.1",
		"prettier-plugin-packagejson": "3.0.0",
		"prettier-plugin-tailwindcss": "0.7.2",
		"tailwindcss": "4.1.18",
		"typescript": "5.9.3",
		"typescript-eslint": "8.56.0",
		"vite": "npm:rolldown-vite@7.3.1",
		"vitest": "4.1.0-beta.4",
		"vitest-plugin-rsc": "0.0.0-experimental-64e35ee-20250807",
		"wrangler": "4.65.0"
	},
	"engines": {
		"node": "24.13.0"
	},
	"icon": "favicon.png",
	"aiaio": {
		"version": ""
	},
	"commands": [
		{
			"name": "aiaio",
			"title": "AIAIO",
			"subtitle": "Build something great.",
			"description": "The Raycast command description.",
			"mode": "view"
		}
	],
	"platforms": [
		"macOS",
		"Windows"
	],
	"title": "AIAIO"
}
